 443:../../../os/rt/include/chlists.h ****   p->prev       = pqp->prev;
 444:../../../os/rt/include/chlists.h ****   p->prev->next = p;
 445:../../../os/rt/include/chlists.h ****   pqp->prev     = p;
 446:../../../os/rt/include/chlists.h **** 
