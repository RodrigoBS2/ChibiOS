 435:../../../os/rt/include/chlists.h **** 
 436:../../../os/rt/include/chlists.h ****   /* Scanning priority queue, the list is assumed to be mostly empty.*/
 437:../../../os/rt/include/chlists.h ****   do {
 438:../../../os/rt/include/chlists.h ****     pqp = pqp->next;
